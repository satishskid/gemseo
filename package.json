{
  "name": "localrank-ai---india-seo-assistant",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run",
    "test:integration": "node --loader ts-node/esm scripts/testArchitecturalImprovements.ts",
    "test:security": "node --loader ts-node/esm scripts/testArchitecturalImprovements.ts --security",
    "test:performance": "node --loader ts-node/esm scripts/testArchitecturalImprovements.ts --performance",
    "test:accessibility": "node --loader ts-node/esm scripts/testArchitecturalImprovements.ts --accessibility",
    "test:smoke": "node --loader ts-node/esm scripts/testArchitecturalImprovements.ts --smoke",
    "deploy:staging": "./scripts/deploy-feature-branch.sh",
    "deploy:rollback": "./scripts/deploy-feature-branch.sh rollback",
    "feature:test": "npm run test:integration && npm run deploy:staging",
    "feature:rollback": "npm run deploy:rollback"
  },
  "dependencies": {
    "@google/genai": "0.15.0",
    "@supabase/supabase-js": "2.44.4",
    "lucide-react": "^0.544.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "resend": "^6.1.0"
  },
  "devDependencies": {
    "@netlify/functions": "^4.2.5",
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
